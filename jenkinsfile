pipeline {
    agent any 
    stages {
        stage('Clone the project') { 
            steps {
                git 'https://github.com/jaswanth3963/hello-world-1.git' 
            }
        }
        stage('build') { 
            steps {
                echo 'this is build test' 
				bat 'mvn clean package'
			}
        }
        stage('Artifact')
	{
	steps{
	archiveArtifacts '**/*.war'
		}
	}
	stage('deploy')
	{
	steps{
	build 'job2'
		}
	    }
	}
    }
